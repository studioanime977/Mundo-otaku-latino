  <script>
    // FunciÃ³n para expandir el reproductor
    function expandPlayer() {
      const videoContainer = document.querySelector('.contenedor-video');
      videoContainer.classList.toggle('expanded');
    }

    // FunciÃ³n para apagar/encender las luces
    function toggleLights() {
      document.body.classList.toggle('lights-off');
    }

    // Deshabilitar clic derecho
    document.addEventListener('contextmenu', function(e) {
      e.preventDefault();
    });

    // Bloquear teclas especÃ­ficas y herramientas de descarga
    document.addEventListener('keydown', function(e) {
      // Bloquear herramientas de desarrollador
      if (e.key === 'F12' || (e.ctrlKey && e.shiftKey && e.key === 'I')) {
        e.preventDefault();
        return false;
      }
      // Bloquear guardar pÃ¡gina y ver cÃ³digo fuente
      if ((e.ctrlKey || e.metaKey) && (e.key === 's' || e.key === 'u')) {
        e.preventDefault();
        return false;
      }
      // Bloquear inspector de elementos
      if (e.ctrlKey && e.shiftKey && e.key.toLowerCase() === 'i') {
        e.preventDefault();
        return false;
      }
      // Bloquear consola de desarrollador
      if (e.ctrlKey && e.shiftKey && e.key.toLowerCase() === 'j') {
        e.preventDefault();
        return false;
      }
      // Bloquear Ctrl+A (seleccionar todo)
      if ((e.ctrlKey || e.metaKey) && e.key.toLowerCase() === 'a') {
        e.preventDefault();
        return false;
      }
      // Bloquear Ctrl+P (imprimir)
      if ((e.ctrlKey || e.metaKey) && e.key.toLowerCase() === 'p') {
        e.preventDefault();
        return false;
      }
    });

    // Bloquear selecciÃ³n de texto
    document.addEventListener('selectstart', function(e) {
      e.preventDefault();
      return false;
    });

    // Bloquear arrastrar elementos
    document.addEventListener('dragstart', function(e) {
      e.preventDefault();
      return false;
    });

    // Detectar herramientas de desarrollador abiertas
    let devtools = {open: false, orientation: null};
    const threshold = 160;
    
    setInterval(() => {
      if (window.outerHeight - window.innerHeight > threshold || 
          window.outerWidth - window.innerWidth > threshold) {
        if (!devtools.open) {
          devtools.open = true;
          // Redirigir o mostrar mensaje de advertencia
          document.body.innerHTML = '<div style="position:fixed;top:0;left:0;width:100%;height:100%;background:#000;color:#fff;display:flex;align-items:center;justify-content:center;font-size:24px;z-index:99999;">Acceso no autorizado detectado</div>';
        }
      } else {
        devtools.open = false;
      }
    }, 500);

    // Bloquear popups y ventanas emergentes de descarga
    window.addEventListener('beforeunload', function(e) {
      // Prevenir descargas automÃ¡ticas
      return;
    });

    // Interceptar intentos de descarga
    document.addEventListener('click', function(e) {
      const target = e.target;
      if (target.tagName === 'A' && target.hasAttribute('download')) {
        e.preventDefault();
        e.stopPropagation();
        return false;
      }
    });

    // Bloquear extensiones de descarga de video comunes
    Object.defineProperty(navigator, 'webdriver', {
      get: () => false,
    });

    // Detectar y bloquear herramientas de descarga
    const originalFetch = window.fetch;
    window.fetch = function(...args) {
      const url = args[0];
      if (typeof url === 'string' && (url.includes('download') || url.includes('save'))) {
        return Promise.reject(new Error('Descarga bloqueada'));
      }
      return originalFetch.apply(this, args);
    };

    // Bloquear XMLHttpRequest para descargas
    const originalXHR = window.XMLHttpRequest;
    window.XMLHttpRequest = function() {
      const xhr = new originalXHR();
      const originalOpen = xhr.open;
      xhr.open = function(method, url, ...args) {
        if (typeof url === 'string' && (url.includes('download') || url.includes('save'))) {
          throw new Error('Descarga bloqueada');
        }
        return originalOpen.apply(this, [method, url, ...args]);
      };
      return xhr;
    };

    // ProtecciÃ³n adicional contra extensiones de descarga
    if (window.chrome && window.chrome.runtime) {
      // Detectar extensiones de Chrome
      window.chrome.runtime = undefined;
    }

    // Bloquear console.log para evitar debugging
    if (typeof console !== 'undefined') {
      console.log = function() {};
      console.warn = function() {};
      console.error = function() {};
      console.info = function() {};
      console.debug = function() {};
    }

    // ProtecciÃ³n contra screenshot y grabaciÃ³n de pantalla
    document.addEventListener('keydown', function(e) {
      // Bloquear Print Screen
      if (e.key === 'PrintScreen') {
        e.preventDefault();
        return false;
      }
      // Bloquear Alt+Print Screen
      if (e.altKey && e.key === 'PrintScreen') {
        e.preventDefault();
        return false;
      }
    });

    // Detectar si la pÃ¡gina estÃ¡ siendo grabada o capturada
    if (navigator.mediaDevices && navigator.mediaDevices.getDisplayMedia) {
      const originalGetDisplayMedia = navigator.mediaDevices.getDisplayMedia;
      navigator.mediaDevices.getDisplayMedia = function() {
        return Promise.reject(new Error('Captura de pantalla bloqueada'));
      };
    }

    function switchPlayer(player) {
      document.querySelectorAll('.player-btn').forEach(btn => btn.classList.remove('active'));
      document.querySelector('#' + player + '-btn').classList.add('active');

      document.querySelectorAll('.player-container').forEach(container => container.classList.remove('active'));
      document.querySelector('#' + player + '-player').classList.add('active');
    }

    document.addEventListener('DOMContentLoaded', () => {
      switchPlayer('player1');
      
      // Social media buttons functionality
      document.getElementById('youtube-btn').addEventListener('click', () => {
        window.open('https://youtube.com/@studiootaku', '_blank');
      });

      document.getElementById('whatsapp-btn').addEventListener('click', () => {
        window.open('https://wa.me/message/STUDIOOTAKU', '_blank');
      });

      document.getElementById('facebook-btn').addEventListener('click', () => {
        window.open('https://facebook.com/studiootaku', '_blank');
      });

      document.getElementById('facebook-group-btn').addEventListener('click', () => {
        window.open('https://facebook.com/groups/studiootaku', '_blank');
      });

      document.getElementById('telegram-channel-btn').addEventListener('click', () => {
        window.open('https://t.me/studiootaku', '_blank');
      });

      document.getElementById('telegram-group-btn').addEventListener('click', () => {
        window.open('https://t.me/studiootakugroup', '_blank');
      });

      document.getElementById('download-app-btn').addEventListener('click', () => {
        window.open('https://app.studiootaku.com', '_blank');
      });
    });
  </script>
